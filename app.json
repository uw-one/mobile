{
  "expo": {
    "name": "UW One",
    "slug": "uwone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logos/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/logos/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#181818"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logos/adaptive.png",
        "backgroundColor": "#181818"
      },
      "package": "com.uwone.mobile"
    },
    "web": {
      "favicon": "./src/assets/logos/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
